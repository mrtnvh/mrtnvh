{
  "rewrites": [
    {
      "source": "/js/stats",
      "destination": "https://plausible.io/js/plausible.js"
    },
    {
      "source": "/api/event",
      "destination": "https://plausible.io/api/event"
    },
    {
      "source": "/projects/:path*",
      "destination": "/activity/:path*",
      "permanent": true
    },
    {
      "source": "/activity",
      "destination": "/",
      "permanent": true
    }
  ]
}
